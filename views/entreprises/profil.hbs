      <div class="form-group">
        
      <h1>{{name}}</h1>
      </div>
      
      <div class="form-row">
        <div class="form-group col-md-6">
         
         <h3>{{email}}</h3>
        </div>
        
      </div>
      <div class="form-group">
        
        <h3>{{address.street}}</h3>
      </div>
      <div class="form-row">
        <div class="form-group col-md-6">
          
